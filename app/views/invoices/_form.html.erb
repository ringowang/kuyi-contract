<%= form_with(model: invoice, local: true) do |form| %>
  <%= render 'shared/error_messages', model: @invoice %>

  <div class="field">
    <%= form.label :start_date %>
    <%= form.date_select :start_date, id: :invoice_start_date %>
  </div>

  <div class="field">
    <%= form.label :end_date %>
    <%= form.date_select :end_date, id: :invoice_end_date %>
  </div>

  <div class="field">
    <%= form.label :due_date %>
    <%= form.date_select :due_date, id: :invoice_due_date %>
  </div>

  <div class="field">
    <%= form.label :total %>
    <%= form.number_field :total, id: :invoice_total %>
  </div>

  <div class="field">
    <%= form.label :renting_phase_id %>
    <%= form.text_field :renting_phase_id, id: :invoice_renting_phase_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
